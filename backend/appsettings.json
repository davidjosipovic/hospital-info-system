{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "DefaultConnection": "Host=${DB_HOST};Port=${DB_PORT};Database=${DB_NAME};Username=${DB_USER};Password=${DB_PASSWORD}"
  },

  "JwtSettings": {
    "SecretKey": "${JWT_SECRET}",
    "Issuer": "YourApp",
    "Audience": "YourUsers",
    "TokenExpirationInMinutes": 60
  }
}
